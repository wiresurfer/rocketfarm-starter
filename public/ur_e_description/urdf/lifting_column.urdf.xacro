<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <!--link name="world" /-->

  <!--joint name="world_joint" type="fixed">
    <parent link="world" />
    <child link = "base_link" />
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
  </joint-->

  <!-- lifting column -->
  <!--xacro:include filename="$(find ur_e_description)/urdf/column.urdf.xacro" /-->
  <joint name="world_joint" type="fixed">
      <parent link="world"/>
      <child link="column_1_link"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

  <link name="column_1_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.5"/>
      <geometry>
        <box size="0.1 0.1 1"/>
        <!--mesh filename="package://ur_e_description/urdf/column_link_1.dae"/-->
      </geometry>
      <material name="LightGrey">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.5"/>
      <geometry>
        <box size="0.1 0.1 1" />
        <!--mesh filename="package://ur_e_description/meshes/ur10e/collision/base.stl"/-->
        <!--mesh filename="package://ur_e_description/urdf/column_link_1.dae"/-->
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <origin xyz="0.0 0 0.5" rpy="0 0 0"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>
  </link>
  <joint name="column_1_joint" type="prismatic">
    <parent link="column_1_link"/>
    <child link="column_2_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.1"/>
    <limit effort="1000.0" lower="0.0" upper="0.98" velocity="0.5"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>
  <link name="column_2_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.499"/>
      <geometry>
        <box size="0.08 0.08 1" />
        <!--mesh filename="package://ur_e_description/meshes/ur10e/visual/base.dae"/-->
      </geometry>
      <material name="LightGrey">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.499"/>
      <geometry>
        <box size="0.08 0.08 1" />
        <!--mesh filename="package://ur_e_description/meshes/ur10e/collision/base.stl"/-->
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <origin xyz="0.0 0 0.5" rpy="0 0 0"/>
      <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>
  </link>
  <joint name="column_2_joint" type="fixed">
    <parent link="column_2_link"/>
    <child link="base_link"/>
    <origin rpy="0 0 0" xyz="0 0 1"/>
  </joint>
</robot>
